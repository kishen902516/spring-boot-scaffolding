{
  "name": "@springboot-cli/generator",
  "version": "1.1.0",
  "description": "A comprehensive CLI tool for generating production-ready Spring Boot microservices with Clean Architecture, Architecture Orchestration, and Agent Learning System",
  "main": "index.js",
  "bin": {
    "springboot-cli": "./bin/cli.js",
    "sbcli": "./bin/cli.js",
    "orchestrator": "./bin/orchestrator-wrapper.js",
    "learning-system": "./bin/learning-wrapper.js",
    "validate-arch": "./bin/validate-wrapper.js"
  },
  "scripts": {
    "postinstall": "node scripts/postinstall.js",
    "test": "node scripts/test-all.js",
    "validate": "./bin/orchestrator.sh validate",
    "validate:fix": "./bin/orchestrator.sh validate --fix",
    "monitor": "./bin/orchestrator.sh continuous .",
    "learning:dashboard": "./bin/agent-learning-system.sh dashboard",
    "learning:report": "./bin/orchestrator.sh report",
    "setup": "node scripts/setup.js"
  },
  "keywords": [
    "spring-boot",
    "cli",
    "generator",
    "microservices",
    "clean-architecture",
    "api-first",
    "openapi",
    "java",
    "maven",
    "scaffold",
    "boilerplate",
    "azure",
    "application-insights",
    "oauth2",
    "event-sourcing",
    "ddd",
    "domain-driven-design",
    "architecture-orchestration",
    "auto-fix",
    "agent-learning",
    "code-validation",
    "architecture-enforcement"
  ],
  "author": "Kishen Sivalingam",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kishen902516/springboot-cli.git"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "os": [
    "linux",
    "darwin",
    "win32",
    "win64"
  ],
  "files": [
    "bin/",
    "templates/",
    "generators/",
    "config/",
    "docs/",
    "scripts/",
    "data/.gitkeep",
    "README.md",
    "LOCAL_USAGE.md",
    "CLAUDE_AGENT_QUICK_START.md"
  ],
  "preferGlobal": true,
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^9.0.0",
    "fs-extra": "^11.0.0",
    "ora": "^5.4.1"
  },
  "devDependencies": {
    "eslint": "^8.0.0"
  }
}
